	<script src="js/jquery-1.11.3.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript" src="jquery.cookie.js"></script>
	<script>
	layui.use('layer', function(){
		var layer = layui.layer;
		$(".userdanwei").html($.cookie("organizationTitle"));
	    $(".username").html($.cookie("realName"));
		$(".leaveload").on('click',function(){
			layer.alert('确定退出登录吗', function(index){
			           $.ajax({
						  		type:"post",
								dataType:'json',
						  		url: ApiUrl+"login/loginout",
						  		async:true,
						  		success:function(res){
						  			if(res.data==1){
						  				
						  				$.cookie("id", null,{ expires: 1 ,path: '/' });//用户id
										$.cookie("realName", null,{ expires: 1 ,path: '/' });//用户姓名
										$.cookie("userCode", null,{ expires: 1 ,path: '/' });//用户编号
										$.cookie("organizationId", null,{ expires: 1 ,path: '/' });//机构ID
										$.cookie("organizationTitle", null,{ expires: 1 ,path: '/' });//机构名称
										$.cookie("deptId",null,{ expires: 1 ,path: '/' });//部门ID
										$.cookie("deptName", null,{ expires: 1 ,path: '/' });//部门名称
										$.cookie("photo", null,{ expires: 1 ,path: '/' });//头像
										$.cookie("jobState", null,{ expires: 1 ,path: '/' });//在职状态
										$.cookie("isStoreman", null,{ expires: 1 ,path: '/' });//是否是保管员(1-是,-1-不是）
										$.cookie("isAdmin", null,{ expires: 1 ,path: '/' });//是否是管理员(1-是,-1-不是）
										$.cookie("adminId", null,{ expires: 1 ,path: '/' });//管理员id
										$.cookie("roleIds", null,{ expires: 1 ,path: '/' });//权限ID
										$.cookie("groupCode",null,{expires:1 ,path: '/'});//集团ID
										window.location.href='login.html';
						  			}
						  		}
							})
                        layer.close(index);
      });
				
		})
	
		$('.rightArrow').on('click',function(){
				if(!$("#top_tabs").is(":animated")){
					$('#top_tabs').stop().animate({'left':40+'px'},function(){
					$('.top_tabs').css('left',40+'px')
					$('.leftArrow').show()
					})
				}
				
		})
		$('.leftArrow').on('click',function(){
			if(!$("#top_tabs").is(":animated")){
				$('#top_tabs').stop().animate({'left':window.sessionStorage.topLeft})
			}
		})
	})
	
	</script>
