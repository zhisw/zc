.title{
    float: left;
    display: block;
    padding: 9px 15px 9px 15px;
    font-weight: 400;
    text-align: right;
}
.follow{
    display: block;
    padding: 9px 5px 9px 5px;
    font-weight: 400;
}
.nine-xs1{
    float: left;
    width: 11.11111111%;
    box-sizing: border-box;
    position: relative;
}
.nine-xs8{
    width: 83.33333333%;
    box-sizing: border-box;
    position: relative;
    float: left;
}
.red-point{
    color:red;
}
.popup-form{
    margin:10px 0 0 0;
}
.search-wrap{
    padding:15px 15px 0 15px;
}
.search-wrap .layui-inline{
    margin:0px 0px 10px 0;
}
/*按钮*/
.search-wrap .btn-box{
    margin-left: 20px;
}
.btn-box{
    margin-left: 20px;
}
.btns-blue{
    height: 30px;
    line-height: 30px;
    background: #6866E9;
    opacity: 0.7;
    filter: alpha(opacity=70);
    border-radius: 15px;
}
.btns-blue:hover{
    opacity: 1;
    filter: alpha(opacity=100);
}
.btns-green{
    height: 30px;
    line-height: 30px;
    background: #46CE4E;
    opacity: 0.7;
    filter: alpha(opacity=70);
    border-radius: 15px;
}
.btns-green:hover{
    opacity: 1;
    filter: alpha(opacity=100);
}
/*树显示*/
.layui-tree li{
    overflow: initial;
}
.categoryTree{
    width: 200px;
    height: 250px;
    border: 1px solid #D2D2D2;
    overflow: auto;
    display: none;
    position: absolute;
    z-index: 99;
    background: rgb(255, 255, 255);
}
.treeWrap{
    position: relative;
}
.printtitle{
    font-size: 18px;
    text-align: center;
    margin: 10px
}
@media print{
    .print-hide{
        display: block!important;
    }
    .layui-table-view{
        display: none;
    }
}
.print-hide{
    display: none;
}
/*隐藏id列*/
td[data-field=id],th[data-field=id]{
    display: none;
}
