	<script>
		(function(doc, win) {
			var docEl = "";
			docEl = doc.documentElement, resizeEvt = "orientationchange" in window ? "orientationchange" : "resize", recalc = function() {
				var clientWidth = docEl.clientWidth;
				if(!clientWidth) {
					return
				} else {
					if(clientWidth > 640) {
						docEl.style.fontSize = 85.33 + "px"
					} else {
						if(clientWidth < 320) {
							docEl.style.fontSize = 42.67 + "px"
						} else {
							if(clientWidth <= 640 || clientWidth >= 320) {
								docEl.style.fontSize = (clientWidth / 750) * 100 + "px"
							}
						}
					}
				}
				//window.localStorage.setItem("rem_px", docEl.style.fontSize);
			};
			if(!doc.addEventListener) {
				return
			}
			win.addEventListener(resizeEvt, recalc, false);
			doc.addEventListener("DOMContentLoaded", recalc, false)
		})(document, window);
	</script>
	<script src="../../layui/layui.js"></script>
	<script src="../../js/peizhi.js"></script>
	<script>
		//获取地址栏参数
		function GetQueryString(name){
		     var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
		     var r = window.location.search.substr(1).match(reg);
		     if(r!=null)return  unescape(r[2]); return null;
		}
		layui.use('jquery',function(){
			var $ = layui.jquery;	
			var propertyId = GetQueryString("id");
			var odId = GetQueryString("orderdetailodId");
			
			$.ajax({           
				type:"get",
				url: ApiUrl + "login/getinfobyorderdetailid",
				dataType:'json',
				xhrFields: {
                     withCredentials: true,
                },
                crossDomain: true,
				data: {
					odId: odId
//					odId: 735
				},
				success:function(data){	
					var str = "";
					str = "<div class='detalis-left fl'>"  
								+"<img src='http://wm-iot.oss-cn-shanghai.aliyuncs.com/"+ data.sp_img +"'>"
							+"</div>"
							+"<div class='detalis-right fl'>"
								+"<p class='detalis-name'>"+ data.sp_mc +"</p>"
								+"<p class='detalis-much'>"
									+"<span>品牌:"+ data.pp_mc +"</span>"
									+"<span>规格:"+ data.model_mc  +"</span>"
								+"</p>"
								+"<p class='detalis-price'>￥"+ data.pt_xsj +"元</p>"
							+"</div>";
					$("#conent .add-detalis").html(str);
					$(".add-company p .name").html(data.supplier_mc);
				}
			});
			
			$.ajax({           
				type:"get",
				url: ApiUrl + "login/getZhpropertybyid",
				dataType:'json',
				xhrFields: {
                     withCredentials: true,
                },
                crossDomain: true,
				data: {
					id: propertyId
//					id: 219
				},
				success:function(data){	
					var myData = data.result;
					var str = ""; 
					if(myData.personnelName == null){
						myData.personnelName = "";
					}
					str = "<div class='info-item clearfix'>"
								+"<p class='fl left'>资产条码：</p>"
								+"<p class='fr right'>"+ myData.propertyCode +"</p>"
						    +"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>资产名称：</p>"
								+"<p class='fr right'>"+ myData.propertyName +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>资产类别：</p>"
								+"<p class='fr right'>"+ myData.categoryName +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>规格型号：</p>"
								+"<p class='fr right'>"+ myData.specification +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>金额：</p>"
								+"<p class='fr right'>"+ myData.price +"元</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>所属公司：</p>"
								+"<p class='fr right'>"+ myData.createOrganizationName +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>入库时间：</p>"
								+"<p class='fr right'>"+ myData.storageinDate +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>存放地点：</p>"
								+"<p class='fr right'>"+ myData.depositAdress +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>使用公司：</p>"
								+"<p class='fr right color-red'>"+ myData.organizationName +"</p>"
							+"</div>"
							+"<div class='info-item clearfix'>"
								+"<p class='fl left'>使用人：</p>"
								+"<p class='fr right color-red'>"+ myData.personnelName +"</p>"
							+"</div>";
							
					$("#conent .part-one").html(str);
				}
			});	
			$.ajax({
				type:"get",
				url: ApiUrl + "login/getZhPropertyLogPage",
				dataType:'json',
				xhrFields: {
                     withCredentials: true,
                },
                crossDomain: true,
				data: {
					propertyId: propertyId
//					propertyId: 219
				},
				success:function(data){	
					var myData = data.data;
					var str1 = ""; 
					//资产处理记录
					for (var i=0; i<myData.length; i++) {
						var statusMethod;  
						if(myData[i].disposeType == 1){
							statusMethod = "入库";
						}
						if(myData[i].disposeType == 2){
							statusMethod = "领用";
						}
						if(myData[i].disposeType == 3){
							statusMethod = "退库";
						}
						if(myData[i].disposeType == 4){
							statusMethod = "借用";
						}
						if(myData[i].disposeType == 5){
							statusMethod = "调拨";
						}
						if(myData[i].disposeType == 6){
							statusMethod = "维修";
						}
						if(myData[i].disposeType == 7){
							statusMethod = "报废";
						}
						if(myData[i].disposeType == 8){
							statusMethod = "回收";
						}
						if(myData[i].disposeType == 9){
							statusMethod = "撤销回收";
						}
						if(myData[i].disposeType == 10){
							statusMethod = "归还";
						}
						if(myData[i].disposeType == 11){
							statusMethod = "调拨中";
						}
						if(myData[i].disposeType == 12){
							statusMethod = "调入确认";
						}
						if(myData[i].disposeType == 13){
							statusMethod = "取消调拨";
						}
						if(myData[i].disposeType == 14){
							statusMethod = "维修完成";
						}
						if(myData[i].disposeType == 15){
							statusMethod = "维修撤销";
						}
						
						str1 += "<div class='process-item'>"
									+"<p class='item-name'>"
										+"<span>"+ myData[i].createat +"</span>"
										+"<span>处理方式："+ statusMethod +"</span>"
										+"<span>处理人："+ myData[i].disposePersonName +"</span>"
									+"</p>"
									+"<div class='item-cont clearfix'>"
										+"<div class='left fl'>处理内容：</div>"
										+"<div class='right fl'>"+ myData[i].describes +"</div>"
									+"</div>"
									+"<span class='blue-dot'></span>"
								+"</div>";
								
						$(".part-two .info-process").html(str1);
					}
				}
			});	
		});
	</script>
